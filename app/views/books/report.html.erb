<h1>Books Report</h1>

<table class="table table-striped">
  <thead class="thead-dark">
    <tr>
      <th>Title</th>
      <th>Release date</th>
      <th>Base price</th>
      <th>Action</th>
  </thead>
  <tbody>
    <% @books.each do |book| %>
<tr>
  <td> <%= book.title %> </td>
  <td> <%= book.release_date %></td>
  <td> $<%= book.base_price %></td>
  <td><%= link_to 'Show', book %></td>
</tr>
 <% end %>
  </tbody>
</table>
